					},
					{
						"name": "DepartmentID",
						"type": "int",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "Salary",
						"type": "decimal",
						"precision": 10,
						"scale": 2
					},
					{
						"name": "HireDate",
						"type": "date",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "LastModifiedDate",
						"type": "datetime",
						"precision": 0,
						"scale": 0
					}
				],
				"typeProperties": {
					"tableName": "employee"
				}
			},
