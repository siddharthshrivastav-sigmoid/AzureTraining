				"schema": [
					{
						"name": "EmployeeID",
						"type": "int",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "FirstName",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "LastName",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "DepartmentID",
						"type": "int",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "Salary",
						"type": "decimal",
						"precision": 10,
						"scale": 2
					},
					{
						"name": "HireDate",
						"type": "date",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "LastModifiedDate",
						"type": "datetime",
						"precision": 0,
						"scale": 0
